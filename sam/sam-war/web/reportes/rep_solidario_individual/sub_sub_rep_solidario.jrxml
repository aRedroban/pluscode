	<parameter name="pide_cliente" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    c.id,
    SUBSTRING_INDEX(fecha_gestion, ' ', 1) AS fecha_gestion,
    lugar_entregaecuenta,
    CASE
        WHEN lugar_entregaecuenta = 'DIGITAL' THEN email
        ELSE email = NULL
    END AS 'email_estado_cuenta',
    tipo_identificacion,
    cedula,
    apellido_paterno,
    apellido_materno,
    CONCAT(primer_nombre, ' ', segundo_nombre) AS nombres,
    concat(apellido_paterno, ' ', apellido_materno, ' ',primer_nombre, ' ', segundo_nombre) as nombres_completos,
    genero,
    estado_civil,
    fecha_nacimiento,
    cargas_familiares,
    nivel_estudios,
    profesion,
    'CEDULA' AS tipo_documento_conyugue,
    identificacion_conyuge,
    apellido1conyuge,
    apellido2conyuge,
    CONCAT(nombre1conyuge, ' ', nombre2conyuge) AS nombres_conyugue,
    pd.nombre as provincia_domicilio,
    cd.nombre AS canton_domicilio,
    pad.nombre as parroquia_domicilio,
    bd.nombre as barrio_domicilio,
    numeracion_domicilio,
    referencia_domicilio,
    calle_principal_domicilio,
    calletransversal_domicilio,
    tipo_vivienda,
    valor_vivienda AS avaluo,
    telefono_principal,
    telefono_celular,
    email,
    origen_ingresos,
    ruc_empresa,
    nombre_labora,
    fecha_ingreso,
    cargo,
    pt.nombre as provincia_trabajo,
    ct.nombre as ciudad_trabajo,
    pat.nombre as parroquia_trabajo,
    bt.nombre as barrio_trabajo,
    numeracion_trabajo,
    referencia_trabajo,
    calle_principal_trabajo,
    calletransversal_trabajo,
    telefono_trabajo,
    apellido1ref_personal,
    apellido2ref_personal,
    CONCAT(nombre1ref_personal, ' ',nombre2ref_personal) AS nombres_referencia,
    telefono_ref_personal,
    sueldo_ingresos AS ingresos_personales,
    rentas,
    remesas,
    ingreso_familiares,
    otros_ingresos,
    gastos_basicos AS alimentacion,
    servicios_basicos,
    transporte,
    otros_gastos,
    activo_bien AS descripcion_activo,
    valor_unitario_bien,
    institucion_pasivo,
    fecha_emision_pasivo,
    valor_cuota_pasivo,
    plazo_pasivo,
    saldo_pasivo,
(SELECT logo_empr FROM sis_empresa) AS logo
FROM
    clientes c
    /*PARA EL DOMICILIO*/
    LEFT JOIN provincia pd on c.ubicacion_provincia_domicilio = pd.id
    LEFT JOIN ciudad cd on c.ubicacion_canton_domicilio = cd.codigo_ubicacion
    LEFT JOIN parroquia pad on c.ubicacion_parroquia_domicilio = pad.codigo_ubicacion
    LEFT JOIN barrio bd on c.ubicacion_barrio_domicilio = bd.codigo_ubicacion
    /*PARA EL TRABAJO*/
    LEFT JOIN provincia pt on c.ubicacion_provincia_trabajo = pt.id
    LEFT JOIN ciudad ct on c.ubicacion_canton_trabajo = ct.codigo_ubicacion
    LEFT JOIN parroquia pat on c.ubicacion_parroquia_trabajo = pat.codigo_ubicacion
    LEFT JOIN barrio bt on c.ubicacion_barrio_trabajo = bt.codigo_ubicacion
WHERE
    c.id = $P{pide_cliente};]]>
	</queryString>
